{
  "name": "rb-vue-cli",
  "version": "0.2.2",
  "description": "simple vue cli",
  "main": "index.js",
  "bin": {
    "rb": "./bin/rb.js"
  },
  "scripts": {
    "prepare": "husky install",
    "commit": "git-cz",
    "lint:eslint": "eslint --fix --ext .js",
    "lint:ls-lint": "ls-lint",
    "lint:lint-staged": "lint-staged -c ./.husky/lint-staged.config.js",
    "release": "release-it"
  },
  "author": {
    "name": "Robin",
    "email": "302740969@qq.com"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^4.0.0",
    "commander": "^9.4.0",
    "download-git-repo": "^3.0.2",
    "figlet": "^1.5.2",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.0.0",
    "ora": "^5.0.0",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@ls-lint/ls-lint": "^1.11.2",
    "@release-it/conventional-changelog": "^5.1.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "release-it": "^15.5.0"
  },
  "lint-staged": {
    "*/.{js,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "git": {
      "commitMessage": "chore(tag): release v${version}"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RobinWM/rb-vue-cli.git"
  }
}
